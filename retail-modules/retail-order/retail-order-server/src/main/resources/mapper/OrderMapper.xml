        #{orderSn},
        #{totalAmount},
        #{integrationAmount},
        #{couponAmount},
        #{freightAmount},
        #{payAmount},
        #{status},
        #{corporationId},
        #{name},
        #{phone},
        #{address},
        #{remark},
        #{couponId},
        #{createTime},
        #{realityTime},
        #{type},
        #{seckillId},
        #{addressId},
        null,
        null

        )
    </insert>
</mapper>
