<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.retail.user.mapper.IntegrationHistoryMapper">

    <select id="selectSign" resultType="com.retail.user.domain.IntegrationHistoryEntity">
        SELECT *from  retail_integration_history
        <where>
            <if test="userId!=null">
                and user_id=#{userId}
            </if>
            <if test="startTime!=null">
                and create_time &gt;=#{startTime}
            </if>
            <if test="endTime!=null">
                and create_time &lt;=#{endTime}
            </if>
        </where>
    </select>
</mapper>
